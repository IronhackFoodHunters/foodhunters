<button class="createbuttonhomepage"> <a class="createbuttonhomepage" href="/private/create-recipe"> CREATE A RECIPE </a> </button>

    {{#each reversed}}

    <h4 class="homepagetexth4">{{title}}</h4>

    <a href="/recipe-details/{{_id}}">
        <img src="{{imageUrl}}" width="200" height="200" alt="{{title}}" />
    </a>

    <h4 class="homepagetexth4">{{owner.username}}</h4>

    <!-- <a href="/profile/{{owner.username}}"> -->

    <img src="{{owner.imageUrl}}" width="100" height="100" alt="{{recipe.owner.username}}">
    <h4 class="homepagetexth4"> {{recipe.owner.username}}</h4>



    <form action="/favourites/{{_id}}" method="POST">
        <button class="likeButton">LIKE</button>
        <!--when this is pushed it need to push the recipe in to recipesLiked-->
    </form>
    {{/each}}
    